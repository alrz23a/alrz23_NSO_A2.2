Host *
	User ubuntu
	IdentityFile ~/.ssh/private_key.pem
	StrictHostKeyChecking no
	PasswordAuthentication no

Host bastionNSO
	HostName 185.52.156.244
	ForwardAgent yes
	ControlMaster auto 
	ControlPath ~/.ssh/ansible-%r@%h:%p
	ControlPersist yes
	
Host HAproxy
	HostName 185.52.156.81
	ForwardAgent yes
	ControlMaster auto 
	ProxyCommand ssh -W %h:%p bastionNSO

Host devA
	HostName 10.0.1.27
	ProxyCommand ssh -W %h:%p bastionNSO

Host devB
	HostName 10.0.1.23
	ProxyCommand ssh -W %h:%p bastionNSO

Host devC
